<?xml version="1.0" encoding="UTF-8"?>
<CLISH_MODULE xmlns="http://clish.sourceforge.net/XMLSchema" 
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:schemaLocation="http://clish.sourceforge.net/XMLSchema
                     http://clish.sourceforge.net/XMLSchema/clish.xsd">
    <!--=======================================================-->
	<PTYPE name="VLAN_ID" 
		pattern="(409[0-5]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{2}|[1-9][0-9]|[1-9])"
		help="Number in the range 1-4095"/>
	<!--=======================================================-->
	<PTYPE name="IP_ADDR"
		pattern="(((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?))"
		help="A.B.C.D"/>
	<!--=======================================================-->
		<PTYPE name="FLOAT"
		pattern="[0-9]+"
		help="Unsigned float"/>
	<!--=======================================================-->
	<PTYPE name="UINT"
		pattern="[0-9]+"
		help="Unsigned integer"/>
	<!--=======================================================-->
	<PTYPE name="STRING"
		pattern=".+"
		help="String"/>
	<!--=======================================================-->
	<PTYPE name="SUBCOMMAND"
		pattern="[^\]+"
		help="String"/>
	<!--=======================================================-->
	<PTYPE name="BOOL"
		method="select"
		pattern="true(1) false(0)"
		help="Boolean choice"/>
	<!--=======================================================-->
	<PTYPE name="IFACE_NUM"
		pattern="[0-9]+"
		help="Interface number"/>
	<!--=======================================================-->
	<PTYPE name="PROTO"
		method="select"
		pattern="ip(ip) ipencap(ipencap) icmp(icmp) igmp(igmp) tcp(tcp) udp(udp)"
		help="Protocol choice"/>
	<!--=======================================================-->
	<PTYPE name="SERVICE"
		pattern="((ftp)|(telnet)|(sntp)|(smtp)|(http)|([0-9]+))"
		help="Port"/>
	<!--=======================================================-->
	<PTYPE name="IP_ADDR_MASK"
		pattern="(((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?))(/(1[0-9]|2[0-9]|3[0-2]|[0-9]))?"
		help="A.B.C.D[/mask]"/>
	<!--=======================================================-->
	<PTYPE name="IP_ADDR_ANY"
		pattern="(((((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?))(/(1[0-9]|2[0-9]|3[0-2]|[0-9]))?)|any)"
		help="A.B.C.D[/mask] or any"/>
	<!--=======================================================-->
	<PTYPE name="PORT_MASK_ANY"
		pattern="^(([0-9]+(/0x[0-9a-f]+)?)|(any))$"
		help="Port[/mask]" />
	<!--=======================================================-->
	<PTYPE name="SEQ"
		pattern="^[0-9]*[1-9][0-9]*"
		help="Entry number"/>
	<!--=======================================================-->
	<PTYPE name="TUNITxiaoshu"
		pattern="^(-(([0-9]+/.[0-9]*[1-9][0-9]*)|([0-9]*[1-9][0-9]*/.[0-9]+)|([0-9]*[1-9][0-9]*)))$"
		help="0 --- -xx.xx"/>
	<!--=======================================================-->
	<PTYPE name="FPORT"
		method="select"
		pattern="port1(1) port2(2) port3(3) port4(4)"
		help="Port choice"/>
	<!--=======================================================-->	
	<PTYPE name="FSPEED"
		method="select"
		pattern="10G(1) 8.5G(2) 6.25G(3) 4.25G(4) 2.5G(5)"
		help="Speed choice"/>
	<!--=======================================================-->	
	<PTYPE name="FSTATE"
		method="select"
		pattern="on(1) off(2) other(3)"
		help="port state"/>
	<!--=======================================================-->	
	<PTYPE name="NSTATE"
		method="select"
		pattern="on(1) off(2)"
		help="port state"/>
	<!--=======================================================-->		
	<PTYPE name="SNMPSTATE"
		method="select"
		pattern="on(1) off(2)"
		help="snmp readonly"/>
	<!--=======================================================-->		
	<PTYPE name="SNMPENABLE"
		method="select"
		pattern="enable(1) disable(2)"
		help="snmp readonly"/>
	<!--=======================================================-->		
	
	<PTYPE name="TRAPENABLE"
		method="select"
		pattern="enable(1) disable(2)"
		help="trap enable"/>
	<!--=======================================================-->		
	
	<PTYPE name="FIBERSTATE"
		method="select"
		pattern="on(1) off(2) auto(3)"
		help="fiber send state"/>
	<!--=======================================================-->	

     <PTYPE name="DDMPOWERMIN"
         method="integer"
          pattern="-50..0"
          help="ddm power"/>
     <PTYPE name="DDMPOWERMAX"
         method="integer"
          pattern="-20..5"
          help="ddm power"/>          
          

     <PTYPE name="DDMVOLTMIN"
         method="integer"
          pattern="3..6"
          help="ddm volt"/>	
     <PTYPE name="DDMVOLTMAX"
         method="integer"
          pattern="3..10"
          help="ddm volt"/>	          

     <PTYPE name="DDMTEMPMIN"
         method="integer"
          pattern="-20..20"
          help="ddm temp"/>	 
     <PTYPE name="DDMTEMPMAX"
         method="integer"
          pattern="30..100"
          help="ddm temp"/>	 

     <PTYPE name="DDMBIASMIN"
         method="integer"
          pattern="3..30"
          help="ddm rxBias"/>	 

     <PTYPE name="DDMBIASMAX"
         method="integer"
          pattern="20..100"
          help="ddm rxBias"/>	 
          

     <PTYPE name="VBUNIT"
         method="integer"
          pattern="2..5"
          help="volt"/>

     <PTYPE name="VXUNIT"
         method="integer"
          pattern="5..20"
          help="volt"/>
          
     <PTYPE name="AUNIT"
         method="integer"
          pattern="-20..10"
          help="temp (-20..10)"/>

     <PTYPE name="APOWL"
         method="integer"
          pattern="-50..10"
          help="power (-50..10)"/>

     <PTYPE name="RETUNT"
         method="integer"
          pattern="0..999"
          help="return time (0..999)"/>          

     <PTYPE name="BUNIT"
         method="integer"
          pattern="0..100"
          help="temp"/>
         
     <PTYPE name="TUNIT"
          pattern="-?[0-9]*.?[0-9]"
          help="temp"/>

     <PTYPE name="SNMPPORT"
         method="integer"
          pattern="1..65535"
          help="temp"/>     

     <PTYPE name="VOLTMIN"
         method="integer"
          pattern="2..10"
          help="volt min"/>     

     <PTYPE name="VOLTMAX"
         method="integer"
          pattern="10..20"
          help="volt max"/>     
     <PTYPE name="VISINTFLOAT"
          pattern="^[- 0-9 .]*$"
          help="float"/>     

          
</CLISH_MODULE>
